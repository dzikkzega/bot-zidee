{
  "name": "zideebot",
  "version": "1.0.0",
  "description": "WhatsApp Bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "start:optimized": "node --max-old-space-size=512 --optimize-for-size --gc-interval=100 index.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop zideebot",
    "pm2:restart": "pm2 restart zideebot",
    "pm2:reload": "pm2 reload zideebot",
    "pm2:delete": "pm2 delete zideebot",
    "pm2:logs": "pm2 logs zideebot",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "cleanup": "node cleanup.js",
    "reset-session": "node reset-session.js",
    "clear": "node clear-session.js",
    "start:clean": "npm run cleanup && npm run start:optimized",
    "start:fresh": "npm run reset-session && npm start",
    "install:panel": "npm install --legacy-peer-deps",
    "install:force": "npm install --force",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:rebuild": "docker-compose up -d --build"
  },
  "keywords": [
    "whatsapp-bot"
  ],
  "author": "",
